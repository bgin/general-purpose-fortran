# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # 
# platform-specific values you will probably change
# 
LIBRARY := libGPF.a
LIB := GPF
LIBS := $(shell bash scripts/if_library_found ncurses tinfo sqlite3 readline X11)
CC := cc
CFLAGS := -O
FC := gfortran
FFLAGS := -O
F90 := gfortran
F90FLAGS := -Wall -Wuninitialized -fbounds-check -fno-range-check -g -I. -J.
LDFLAGS := -Wl,--allow-multiple-definition -fno-range-check -Wall -fbounds-check -g -I. -J.
# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # 
all: $(PROG) $(CPROG)
	test -e scripts/test_suite && chmod u+xr scripts/test_suite||echo "test_suite not found"
	PATH="`pwd`/scripts:$$PATH"; which test_suite && test_suite -l test_suite_log.txt

$(PROG): $(LIBRARY)

	-$(F90) $(LDFLAGS) $@.f90 -L. -l$(LIB) -o $@ $(LIBS)

$(LIBRARY): $(OBJS)
	$(AR) $(ARFLAGS) $@ $^

clean:
	rm -f $(PROG) $(CPROG) $(OBJS) *.mod

.SUFFIXES: $(SUFFIXES) .f90 .F90 .ff .FF .shf
# .shf -- assumed to write Fortran code to stdout when executed
# .FF -- run thru ufpp(1) with    $system directives allowed
# .ff -- run thru ufpp(1) without $system directives allowed

.f90.o:
	$(F90) $(F90FLAGS) -c $<

.F90.o:
	$(F90) $(F90FLAGS) -c $<

.f95.o:
	$(F90) $(F90FLAGS) -c $<

.F95.o:
	$(F90) $(F90FLAGS) -c $<

.f03.o:
	$(F90) $(F90FLAGS) -c $<

.F03.o:
	$(F90) $(F90FLAGS) -c $<
#=================================================================================
# Fortran free format file known to have ufpp(1) preprocessor directives
# run thru ufpp(1) preprocessor with system commands allowed, variable F90 defined
# Assumes .F90 file does not exist previously, as it will overwrite it.
.FF.F90:
	@# run thru ufpp(1) preprocessor with system commands allowed
	ufpp -D F90 `uname -o` -verbose -system .true. -i $(<) -o $(*F).F90
	@[ -s $(*F).F90 ] || echo "error: $(*F).F90 is empty"
#=================================================================================
# Fortran free format file known to have ufpp(1) preprocessor directives
# run thru ufpp(1) preprocessor with no system commands allowed, variable F90
# defined. Assumes .F90 file does not exist previously, as it will overwrite it.
.ff.f90:
	@# run thru ufpp(1) preprocessor with system commands allowed
	ufpp -D F90 `uname -o` -verbose -i $(<) -o $(*F).F90
	@[ -s $(*F).F90 ] || echo "error: $(*F).F90 is empty"
#=================================================================================

