all: $(PROG) $(CPROG)
	test -e scripts/test_suite && chmod u+xr scripts/test_suite||echo "test_suite not found"
	env PATH="`pwd`/scripts:$$PATH" which test_suite && test_suite -l test_suite_log.txt

$(PROG): $(LIBRARY)

	$(F90) $(LDFLAGS) $@.f90 -L. -l$(LIB) -o $@ $(LIBS)

